<%= form_for @survey, layout: :basic do |f| %>
  <%= f.text_field :name %>
  <%= f.select(:store, [['4453', 4453], ['11785', 11785], ['11960', 11960], ['11785', 11785]]) %>
  <%= f.select(:fc_dt, [['FC', 'FC'], ['DT', 'DT']]) %>
  <%= f.text_field :api_user %>
  <%= f.text_field :api_user_key %>  

  <%= f.submit 'Save' %>

<% end %>
